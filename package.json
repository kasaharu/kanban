{
  "name": "kanban",
  "version": "10.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run prebuild",
    "start": "ng serve",
    "prebuild": "ts-node scripts/prebuild.ts && ts-node scripts/prebuild.ts prod",
    "build": "ng build",
    "test": "ng test --code-coverage --watch false --progress false",
    "test:ci": "npm run test -- --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "stylelint": "stylelint './**/*.scss'",
    "format": "prettier --write './**/*.{ts,html,scss}'",
    "format:check": "prettier --check './**/*.{ts,html,scss}'"
  },
  "private": true,
  "dependencies": {
