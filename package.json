    "@angular/cli": "17.2.2",
    "@angular/compiler-cli": "17.2.3",
    "@angular/language-service": "17.2.3",
    "@types/jasmine": "4.6.1",
    "@types/jasminewd2": "2.0.12",
    "@types/node": "18.18.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "codelyzer": "6.0.2",
    "ejs": "3.1.9",
    "eslint": "^8.53.0",
    "jasmine-core": "5.1.2",
